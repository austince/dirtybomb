{
  "name": "dirtybomb",
  "version": "1.0.2",
  "description": "A dirty bomb model",
  "main": "dist/Dirtybomb.js",
  "directories": {
    "src": "src",
    "test": "test"
  },
  "scripts": {
    "clean": "node_modules/.bin/gulp clean",
    "prebuild": "npm run clean && echo Building into build/ and dist/",
    "build": "sh build.sh",
    "postbuild": "node_modules/.bin/gulp minify-dist",
    "doc": "node_modules/.bin/esdoc -c esdoc.json",
    "servedocs": "node_modules/.bin/hs docs",
    "pretest": "npm run build",
    "test": "node_modules/.bin/mocha --compilers js:babel-core/register",
    "testcov": "NODE_ENV=test DIRTYBOMB_COVERAGE=1 node_modules/.bin/mocha --compilers js:babel-core/register --require blanket --reporter mocha-lcov-reporter | node_modules/coveralls/bin/coveralls.js",
    "prepublish": "npm run build && npm run test",
    "lint": "node_modules/.bin/eslint ."
  },
  "config": {
    "blanket": {
      "pattern": [
        "build/Dirtybomb.js"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/austincawley/dirtybomb.git"
  },
  "keywords": [
    "Dirtybomb",
    "Terrorism",
    "Gaussian"
  ],
  "author": " <austin.cawley@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/austincawley/dirtybomb/issues"
  },
  "homepage": "https://github.com/austincawley/dirtybomb#readme",
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-core": "^6.7.4",
    "babel-preset-es2015": "^6.9.0",
    "blanket": "^1.2.3",
    "chai": "^3.5.0",
    "coveralls": "^2.11.9",
    "esdoc": "^0.4.7",
    "eslint": "^2.13.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.9.2",
    "eslint-plugin-jsx-a11y": "^1.5.3",
    "eslint-plugin-react": "^5.2.2",
    "gulp": "^3.9.1",
    "gulp-header": "^1.8.8",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-uglify": "^2.0.0",
    "http-server": "^0.9.0",
    "mocha": "^2.5.3",
    "mocha-lcov-reporter": "^1.2.0",
    "rollup": "^0.26.3",
    "rollup-plugin-babel": "^2.4.0"
  }
}
